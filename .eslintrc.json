{
  "extends": "airbnb-base",
  "rules": {
    "no-var": 0,
    "prefer-arrow-callback": 0,
    "object-curly-spacing": 0,
    "object-shorthand": [2, "consistent"],
    "comma-dangle": [2, "never"],
    "no-unused-vars": [1, {"vars": "local", "args": "none"}],
    "vars-on-top": 0
  },
  "env": {
    "browser": true,
    "jquery": true
  }
}
