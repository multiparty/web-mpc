{
  "extends": "eslint:recommended",
  "rules": {
    "indent": [2, 2],
    "object-shorthand": [2, "consistent"],
    "no-unused-vars": [1, {"vars": "local", "args": "none"}],
    "quote-props": [1, "consistent-as-needed"],
    "eqeqeq": 2,
    "brace-style": 2,
    "curly": 2
  },
  "env": {
    "browser": true,
    "jquery": true
  }
}
